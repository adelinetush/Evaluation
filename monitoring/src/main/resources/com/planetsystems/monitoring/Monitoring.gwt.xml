<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='Monitoring'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.inject.Inject' />
	<inherits name='com.google.gwt.resources.Resources' />
	<inherits name="com.google.gwt.uibinder.UiBinder" />
	
	

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<!-- Other module inherits -->
	<inherits name='com.gwtplatform.mvp.Mvp' />
	<inherits name='com.gwtplatform.dispatch.Dispatch' />
	<inherits name='net.sf.gilead.Gilead4Gwt' />
	<inherits name='com.planetsystems.monitoring.MonitoringModel' /> 
	
	<inherits name="com.smartgwt.SmartGwtNoTheme" />
	<inherits name="com.smartclient.theme.enterpriseblue.EnterpriseBlue" />
	<inherits name="com.smartclient.theme.enterpriseblue.EnterpriseBlueResources" />

	<!-- Specify the app entry point class. -->
	 <entry-point class='com.planetsystems.monitoring.client.Monitoring' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />
	<define-configuration-property name='gin.ginjector'
		is-multi-valued='false' />
	<set-configuration-property name='gin.ginjector' value='com.planetsystems.monitoring.client.gin.ClientGinjector' />

</module>
